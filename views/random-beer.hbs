<div class="container">
    <h1>Random beer</h1>
    <div class="row">
        {{#each randomPartial}}
            {{>beerpartial}}
            <ul>
                <span>Food Pairing</span>
                {{#each food_pairing}}
                    <li>{{this}}</li>
                {{/each}}
            </ul>
            <p class="card-text">
                <span>Brewers tips:</span>
                {{brewers_tips}}
            </p>
        {{/each}}
    </div>
</div>